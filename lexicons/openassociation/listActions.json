{
  "lexicon": 1,
  "id": "org.openassociation.listActions",
  "defs": {
    "main": {
      "type": "query",
      "description": "List actions. Filterable by: actionId, inputOutput, createResource, eventQuantity, accountingEffect, onhandEffect, locationEffect, containedEffect, accountableEffect, stageEffect, stateEffect.",
      "parameters": {
        "type": "params",
        "properties": {
          "uri": {
            "type": "string",
            "format": "at-uri",
            "description": "Fetch a single record by AT-URI. Other filters are ignored when set."
          },
          "actionId": {
            "type": "string",
            "description": "Filter by actionId value."
          },
          "inputOutput": {
            "type": "string",
            "description": "Filter by inputOutput value."
          },
          "createResource": {
            "type": "string",
            "description": "Filter by createResource value."
          },
          "eventQuantity": {
            "type": "string",
            "description": "Filter by eventQuantity value."
          },
          "accountingEffect": {
            "type": "string",
            "description": "Filter by accountingEffect value."
          },
          "onhandEffect": {
            "type": "string",
            "description": "Filter by onhandEffect value."
          },
          "locationEffect": {
            "type": "string",
            "description": "Filter by locationEffect value."
          },
          "containedEffect": {
            "type": "string",
            "description": "Filter by containedEffect value."
          },
          "accountableEffect": {
            "type": "string",
            "description": "Filter by accountableEffect value."
          },
          "stageEffect": {
            "type": "string",
            "description": "Filter by stageEffect value."
          },
          "stateEffect": {
            "type": "string",
            "description": "Filter by stateEffect value."
          },
          "limit": {
            "type": "integer",
            "minimum": 1,
            "maximum": 100,
            "default": 50,
            "description": "Maximum number of records to return."
          },
          "cursor": {
            "type": "string",
            "description": "Pagination cursor from a previous response."
          }
        }
      },
      "output": {
        "encoding": "application/json"
      }
    }
  }
}
